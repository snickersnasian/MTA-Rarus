{
    "manifest_version": 2,
    "name": "MTA-Rarus",
    "description": "Автоматическая загрузка принтов на сервисы мерчандайзинга",
    "version": "0.0.1",
    "author":"zuizun",
    "browser_action": {
        "default_title":"MTA-Rarus",
        "default_icon":"src/icon.png",
        "default_popup":"index.html"
    },
    "content_scripts": [{
        "matches":["<all_urls>"],
        "js": ["content.js"]
    }]
}